// bs2

// Installing MOS
ACTION_IF GAME_IS ~pstee~ BEGIN
COPY ~%MOD_FOLDER%/copy/mos+pvrz/g-btbs2.mos~ ~override~
  LPF UPDATE_PVRZ_INDICES
    RET
      original_base_index
      new_base_index
  END

ACTION_IF (new_base_index >= 0) BEGIN
  // Installing PVRZ resources
  ACTION_FOR_EACH file IN ~MOS0000.pvrz~ ~MOS0001.pvrz~ ~MOS0002.pvrz~ ~MOS0003.pvrz~ BEGIN
    LAF INSTALL_PVRZ
      INT_VAR
        original_base_index
        new_base_index
      STR_VAR
        source_file = EVAL ~%MOD_FOLDER%/copy/mos+pvrz/%file%~
      RET
        success
		DEST_FILE
    END
    ACTION_IF (success) BEGIN //ACTION_IF (NOT success) BEGIN
	 PRINT ~Successfully installed file %file% as %DEST_FILE%.~
	 END ELSE BEGIN // else begin
      WARN ~Could not install file %file%!~
    END
  END
END
END

